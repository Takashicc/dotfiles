{
  "background.fullscreen": {
    "images": ["file:///Users/tak/Downloads/pics/72525842_p0.jpg"],
    "opacity": 0.1,
    "size": "cover",
    "position": "center",
    "interval": 0,
    "random": false
  },
  "workbench.colorTheme": "Ayu Mirage Zed",
  "workbench.iconTheme": "eq-material-theme-icons-light",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.wordWrap": "on",
  "editor.stickyScroll.enabled": true,
  "gitlens.telemetry.enabled": false,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "yaml.format.enable": true,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence",
    "!anystr",
    "!anyint",
    "!int",
    "!str",
    "!anybool"
  ],
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnSave": true
  },
  "[xml]": {
    "editor.formatOnSave": true
  },
  "xml.format.enabled": true,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnSave": true
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.formatOnSave": true
  },
  "markdownlint.run": "onSave",
  "markdownlint.config": {
    "MD014": false
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "go.goroot": "~/.goenv/versions/1.24.0",
  "[csharp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "todo-tree.general.tags": ["TODO", "NOTE", "WARN", "FIXME"],
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "#000000",
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check-circle",
      "iconColour": "#00FF00",
      "background": "#00FF00"
    },
    "NOTE": {
      "icon": "thumbsup",
      "iconColour": "#1E90FF",
      "background": "#1E90FF"
    },
    "WARN": {
      "icon": "alert",
      "iconColor": "#f11e1e",
      "background": "#f11e1e"
    },
    "FIXME": {
      "icon": "flame",
      "iconColor": "#FFD700",
      "background": "#FFD700"
    }
  },
  "cSpell.ignorePaths": [],
  "cSpell.ignoreWords": ["anystr", "anyint", "anybool"],
  "githubPullRequests.pullBranch": "never",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "diffEditor.ignoreTrimWhitespace": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "go.toolsManagement.autoUpdate": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "files.associations": {
    "*.tmpl": "html"
  },
  "cSpell.userWords": ["Iden", "tagpr", "Taskfile"],
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "animations.Install-Method": "Custom CSS and JS",
  "animations.Enabled": true,
  "animations.CursorAnimation": true,
  "apc.imports": [],
  "vscode_custom_css.imports": [
    "file:///Users/tak/.cursor/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],
  "git.autofetch": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "Cargo.toml": "Cargo.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "git.confirmSync": false
}
